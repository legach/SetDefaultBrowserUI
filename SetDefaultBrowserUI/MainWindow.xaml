<Window x:Class="SetDefaultBrowserUI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:SetDefaultBrowserUI"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="300">
    <StackPanel>
        <Button Width="70" Height="25" Margin="10" Click="SetBrowser_OnClick">
            <TextBlock Text="Set chrome"></TextBlock>
        </Button>
        <Button Width="70" Height="25" Click="GetList_OnClick">
            <TextBlock Text="Get list"></TextBlock>
        </Button>
    </StackPanel>
</Window>
